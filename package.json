{
  "name": "dex-backend",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "prebuild": "npx rimraf dist",
    "build": "npx nest build",
    "format": "npx prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npx nest start",
    "start:dev": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node ./dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:dev:create": "npx prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "npx prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "gen:types": "npx ts-node ./generate-typings.ts",
    "codegen": "npx graphql-codegen --config codegen.yml -r dotenv/config"
  },
  "dependencies": {
    "@balancer-labs/sdk": "^0.1.40",
    "@balancer-labs/sor": "^4.0.1-beta.16",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@graphql-tools/schema": "^8.3.1",
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.1.6",
    "@nestjs/platform-express": "^8.0.0",
    "@prisma/client": "^4.7.1",
    "apollo-server-express": "^3.11.1",
    "axios": "^0.24.0",
    "bignumber.js": "^9.1.0",
    "cache-manager": "^4.1.0",
    "compression": "^1.7.4",
    "date-fns": "^2.29.3",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "fs-extra": "^11.1.0",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "helmet": "^5.1.1",
    "limiter": "^2.1.0",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.39",
    "nestjs-prisma": "^0.19.0",
    "node-cron": "^3.0.2",
    "node-schedule": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-morph": "^17.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/schema-ast": "^2.4.0",
    "@graphql-codegen/typescript": "2.4.0",
    "@graphql-codegen/typescript-document-nodes": "^2.2.0",
    "@graphql-codegen/typescript-graphql-request": "^4.3.0",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@graphql-codegen/typescript-resolvers": "2.4.1",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "27.5.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.0.0",
    "@types/node-schedule": "^2.1.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "prisma": "^4.7.1",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
